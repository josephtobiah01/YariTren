                        <ColumnDefinition Width="3*" />
                        <ColumnDefinition Width="7*" />
                        <ColumnDefinition Width="3*" />
                    </Grid.ColumnDefinitions>

                    <Image Grid.Column="0" x:Name="leftArrow" HorizontalOptions="Start" HeightRequest="50" WidthRequest="80" Aspect="AspectFit" >
                        <Image.Triggers>
                            <DataTrigger TargetType="Image" Binding="{Binding IsMonthLeftArrow}" Value="True">
                                <Setter Property="Source" Value="arrowinactiveleft" />
                                <Setter Property="IsEnabled" Value="False" />
                            </DataTrigger>
                            <DataTrigger TargetType="Image" Binding="{Binding IsMonthLeftArrow}" Value="False">
                                <Setter Property="Source" Value="arrowactiveleft" />
                                <Setter Property="IsEnabled" Value="True" />
                            </DataTrigger>
                        </Image.Triggers>
                        <Image.GestureRecognizers>
                            <TapGestureRecognizer Tapped="Handle_Tapped_Left" />
                        </Image.GestureRecognizers>
                    </Image>
                    <Label Grid.Column="1" x:Name="monthName" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" FontSize="20" TextColor="Black" FontFamily="{OnPlatform Android=Montserrat-Medium, iOS=Montserrat-Medium}"/>

                    <Image Grid.Column="2" x:Name="rightArrow" HorizontalOptions="EndAndExpand" HeightRequest="50" WidthRequest="80" Aspect="AspectFit">
                        <Image.Triggers>
                            <DataTrigger TargetType="Image" Binding="{Binding IsMonthRightArrow}" Value="True">
                                <Setter Property="Source" Value="arrowinactiveright" />
                                <Setter Property="IsEnabled" Value="False" />
                            </DataTrigger>
                            <DataTrigger TargetType="Image" Binding="{Binding IsMonthRightArrow}" Value="False">
                                <Setter Property="Source" Value="arrowactiveright" />
                                <Setter Property="IsEnabled" Value="True" />
                            </DataTrigger>
                        </Image.Triggers>
                        <Image.GestureRecognizers>
                            <TapGestureRecognizer Tapped="Handle_Tapped_Right" />
                        </Image.GestureRecognizers>
                    </Image>
                </Grid>


